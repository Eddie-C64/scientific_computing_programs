PROGRAM functions
real :: x(4)
x = (/1.8, 1.2, -1.2,-1.8/)
print *, "These are values of x: " 
print *, x
print *, "This is the floor output of x: " 
print *, floor(x)
print *, "This is the ceiling output of x: " 
print *, ceiling(x)
print *, "This is the nint output of x: " 
print *, nint(x)
print *, "This is the int output of x: " 
print *, int(x)
END PROGRAM functions